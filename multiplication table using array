
#include <stdio.h>

int main() {
    int n;
    int arr[10];
    
    printf("enter the number\n");
    scanf("%d",&n);
    
    printf("the table of %d is:\n",n);
    for(int i=0;i<10;i++){
        
        int m = n*(i+1);
     arr[i]=m;
    }
    for(int i=0;i<10;i++){
        printf("%d x %d = %d\n",n,i+1,arr[i]);
    }
    
    return 0;
}
