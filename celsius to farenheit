#include <stdio.h>
float farenheit(float c);
int main() {
    float c;
    
    printf("enter the teperature in celsius:\n");
    scanf("%f",&c);
    
    printf(" teperature in farenheit is : %.1fÂ°F", farenheit(c));
    return 0;
