#include <stdio.h>

int main() {
    int n;
    int marks[n];
    printf("enter the number of subjects : ");
    scanf("%d",&n);
    
    for(int i=0;i<n;i++){
        printf("enter the subject %d marks\n",i+1);
        scanf("%d",&marks[i]);
    }
    
    for(int i=0; i<n;i++){
        printf("the value of subject %d is : %d\n",i+1,marks[i]);
    }
    
    return 0;
}
