
#include <stdio.h>
int fibonacci(int n);
int main() {
int num;
    printf("enter the value\n");
    scanf("%d",&num);
    
    printf("the fibonacci value of %d is: %d",num,fibonacci(num));
    
    return 0;
}
int fibonacci(int n){
    int result;
    if(n==1){
        return 0;
    }
    else if(n==2){
        return 1;
    }
    else{
        result = (n-1+n-2);
        return result;
    }
    
}
