#include <stdio.h>

int main() {
   int n;
   int arr[100];
   
  printf("enter the number of elemrnts\n");
  scanf("%d",&n);
   
   for(int i=0;i<n;i++){
       printf("enter the %d number : \n",i+1);
       scanf("%d",&arr[i]);
   }
    for(int i=0;i<n;i++){
        arr[0]<arr[i];
        arr[0]=arr[i];
    }
    printf("%d is maxmium\n",arr[0]);
    return 0;
}
