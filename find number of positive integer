
#include <stdio.h>
int negative(int*arr,int n){
     int f=0;
    for(int i=0;i<n;i++){
     if(arr[i]>0){
           f++;
     }
   }
     printf("%d numbers are postive",f);
}
int main() {
    int n;
    printf("enter the number of elements\n");
    scanf("%d",&n);
    int arr[n];
    
   for(int i=0;i<n;i++){
       printf("enter the %d numbers: \n",i+1);
       scanf("%d",&arr[i]);
   }
 negative(arr,n);
    return 0;
}
