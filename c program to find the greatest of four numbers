#include <stdio.h>

int main() {
     
    int a,b,c,d;
    
    printf("enter first number\n");
    scanf("%d",&a);
    
    printf("enter second number\n");
    scanf("%d",&b);
    
    printf("enter third number\n");
    scanf("%d",&c);
    
    printf("enter fourth number\n");
    scanf("%d",&d);
    
    if( (a>b) && (a>c) && (a>d) ){
         printf("%d is grestest\n",a);
    }
     if( (b>a) && (b>c) && (b>d) ){
         printf("%d is grestest\n",b);
    }
     if( (c>a) && (c>b) && (c>d) ) {
         printf("%d is grestest\n",c);
    }
     if( (d>a) && (d>b) && (d>c) ){
         printf("%d is grestest\n",d);
    }

     
    return 0;
}
