#include <stdio.h>

int main() {
    
   int n;
   int prime =0;
   
   printf("enter the number\n");
   scanf("%d",&n);
   
   for(int i=2; i*i<=n ; i++){
       
       if(n%i ==0){
           prime++;
           break;
       }
       
   }
   if(prime == 0){
       printf("%d  is a prime number",n);
   }
  else{
      printf("%d is not a prime number",n);
  }
   
    return 0;
}
